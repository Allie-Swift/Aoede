{
    "name": "Aoede",
    "version": "0.1.0",
    "private": true,
    "homepage": "./",
    "main": "public/main.js",
    "description": "A music player",
    "repository": "https://github.com/Allie-Swift/Aoede.git",
    "author": "lhinxue <lhinxue@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@ant-design/icons": "^5.1.4",
        "@electron/remote": "^2.0.10",
        "@radix-ui/react-scroll-area": "^1.0.4",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "antd": "^5.7.2",
        "better-sqlite3": "^8.4.0",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "electron-is-dev": "^2.0.0",
        "express": "^4.18.2",
        "ip": "^1.1.8",
        "node-id3": "^0.2.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "concurrently -k \"cross-env BROWSER=none yarn react-scripts start\" \"yarn electron .\"",
        "build-win": "yarn react-scripts build && electron-builder -w -c.extraMetadata.main=build/main.js",
        "build-mac": "yarn react-scripts build && electron-builder -m -c.extraMetadata.main=build/main.js"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^8.2.0",
        "cross-env": "^7.0.3",
        "electron": "^25.3.0",
        "electron-builder": "^24.4.0",
        "wait-on": "^7.0.1"
    },
    "build": {
        "productName": "Aeode",
        "appId": "com.allie_swift.aeode",
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "public"
        },
        "mac": {
            "target": "dmg"
        },
        "win": {
            "target": "nsis"
        },
        "linux": {
            "target": "deb"
        }
    }
}
